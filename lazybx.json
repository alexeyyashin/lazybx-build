{
  "root": ".",
  "namespace": "MyRootNamespace",
  "entity": {
    "path": "{{root}}/lib/Entity",
    "namespace": "{{namespace}}\\Entity",
    "parent": "\\AbstractEntity",
    "defaultCollection": "\\DefaultCollection"
  },
  "model": {
    "path": "{{root}}/lib/Model",
    "namespace": "{{namespace}}\\Model",
    "parent": "\\AbstractModel"
  },
  "blueprint": {
    "root": "{{root}}/blueprint",
    "path": "{{blueprint.root}}/data",
    "artifacts": "{{blueprint.root}}/artifacts",
    "parent": "\\AbstractBlueprint",
    "fieldParent": "\\AbstractFieldBlueprint",
    "autoloader": "{{blueprint.root}}/path/to/autoloader.php",
    "customTemplatePath": null
  },
  "meta": {
    "generate": true,
    "collection": {
      "template": null,
      "path": "{{root}}/meta/Collection",
      "namespace": "{{namespace}}\\Meta\\Collection"
    }
  },
  "trim": ["words", "to", "exclude", "during", "code", "generation"]
}
